# AEGIS Environment Variables
# Copy to .env and update values

# Teams Webhook for HERALD notifications
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your-webhook-url

# ServiceNow Instance
SNOW_INSTANCE=https://myaccortrain.service-now.com
SNOW_USER=aegis_api_user
SNOW_PASSWORD=your_password_here

# OpenAI for SHERLOCK agent
OPENAI_API_KEY=sk-your-key-here

# Redis (defaults for Docker network)
REDIS_HOST=redis
REDIS_PORT=6379

# n8n Authentication
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=aegis2026

# Governance Defaults
AEGIS_MODE=assist
AEGIS_KILLSWITCH=true
